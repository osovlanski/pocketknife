{
  "name": "pocketknife-backend",
  "version": "2.0.0",
  "description": "Pocketknife - Multi-agent AI platform for emails, jobs, travel, and learning",
  "main": "src/index.ts",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "prisma generate && tsc",
    "clean": "rimraf dist",
    "lint": "tsc --noEmit",
    "auth:gmail": "tsx src/scripts/getGmailTokens.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@prisma/client": "^7.2.0",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "googleapis": "^144.0.0",
    "ioredis": "^5.8.2",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "prisma": "^7.2.0",
    "socket.io": "^4.8.1",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^20.14.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "nodemon": "^3.1.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
